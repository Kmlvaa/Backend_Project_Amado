@model Backend_Project_Amado.Areas.Admin.Models.CheckoutIndexVM

    <!-- Table Start -->
    <div class="container-fluid pt-4 px-4">
                <div class="row g-4">
                    <div class="col-12">
                        <div class="bg-secondary rounded h-100 p-4">
                            <h6 class="mb-4">Person Details</h6>
                            <div class="table-responsive">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th scope="col">#</th>
                                            <th scope="col">First Name</th>
                                            <th scope="col">Last Name</th>
                                            <th scope="col">Email</th>
                                            <th scope="col">Phone Number</th>
                                            <th scope="col">Company Name</th>
                                            <th scope="col">ZIP</th>
                                            <th scope="col">Status</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                @foreach(var checkout in Model.Checkouts)
                                {
                                    <tr>
                                        <th scope="row">@checkout.Id</th>
                                        <td>@checkout.FirstName</td>
                                        <td>@checkout.LastName</td>
                                        <td>@checkout.EmailAddress</td>
                                        <td>@checkout.PhoneNumber</td>
                                        <td>@checkout.CompanyName</td>
                                        <td>@checkout.ZipCode</td>
                                        <td>Member</td>
                                    </tr>
                                }
                            </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
    <!-- Table End -->
 
